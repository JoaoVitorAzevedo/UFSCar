INSERT INTO T_REQUIREMENT VALUES(28,'2012-02-27 16:32:00.974000','ConsultarAbatedor','C\u00f3digo, Raz\u00e3o Social, CNPJ, CPF, Nome, Rua, N\u00famero, Complemento, CEP, Bairro, Telefone, Celular, E-mail','O funcion\u00e1rio deve solicitar a ferramenta  a pesquisa de um abatedor. O funcion\u00e1rio deve preencher todos os campos de entrada exibidos na interface e mostrados no quadro de entradas. O funcion\u00e1rio solicita a consulta dos dados. Quando isso ocorrer o sistema mostra os dados do abatedor na tela. A qualquer momento o funcion\u00e1rio pode escolher a op\u00e7\u00e3o de voltar ao menu. Quando isso ocorrer, deve ser exibida a interface incial da ferramenta.','Nenhuma.',6,1,1,1,NULL,2)
INSERT INTO T_REQUIREMENT VALUES(29,'2012-05-15 15:37:50.674000','Cadastrar Cliente 1','Dados do cliente salvos no banco de dados da aplica\u00e7\u00e3o.','O funcion\u00e1rio deve solicitar a ferramenta a inclus\u00e3o de um novo cliente. O funcion\u00e1rio deve preencher todos os campos de entrada exibidos na interface e mostrados no quadro de entradas. O funcion\u00e1rio solicita o salvamento dos dados. Quando isso ocorrer o sistema grava a data e a hora do cadastro automaticamente. A ferramenta deve atribuir, automaticamente, um n\u00famero de identifica\u00e7\u00e3o (c\u00f3digo) autoincremental ao cliente. A qualquer momento o funcion\u00e1rio pode escolher a op\u00e7\u00e3o de cancelar o cadastro. Quando isso ocorrer, deve ser exibida a interface incial da ferramenta.','O campo c\u00f3digo deve ser \u00fanico e gerado automaticamente.',1,1,1,1,30,2)
INSERT INTO T_REQUIREMENT VALUES(30,'2012-05-15 15:38:12.936000','Cadastrar Cliente','Dados do cliente salvos no banco de dados da aplica\u00e7\u00e3o.','O funcion\u00e1rio deve solicitar a ferramenta a inclus\u00e3o de um novo cliente. O funcion\u00e1rio deve preencher todos os campos de entrada exibidos na interface e mostrados no quadro de entradas. O funcion\u00e1rio solicita o salvamento dos dados. Quando isso ocorrer o sistema grava a data e a hora do cadastro automaticamente. A ferramenta deve atribuir, automaticamente, um n\u00famero de identifica\u00e7\u00e3o (c\u00f3digo) autoincremental ao cliente. A qualquer momento o funcion\u00e1rio pode escolher a op\u00e7\u00e3o de cancelar o cadastro. Quando isso ocorrer, deve ser exibida a interface incial da ferramenta.','O campo c\u00f3digo deve ser \u00fanico e gerado automaticamente.',1,1,1,1,NULL,3)
